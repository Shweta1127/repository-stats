{
  "compilerOptions": {
    "noImplicitAny": false,
    "strict": true,
    "noUnusedLocals": false,
    "noFallthroughCasesInSwitch": true,
    "target": "es2019",
    "moduleResolution": "node",
    "removeComments": true,
    "module": "commonjs",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "outDir": "./dist",
    "resolveJsonModule": true
  },
  "include": [
    "./**/*.ts"
  ]
}
